@model Invitaciones.Shared.Data.MenuUsuariosTipo

@{
    ViewData["Title"] = "Delete";
}

<div class="card ">
<div class="card-header">
    <h3 class="card-title">Menú Tipo Usuario</h3>
</div>
    <dl class="card-body">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdMenuNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdMenuNavigation.MenuTitulo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdUsuarioTipoNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdUsuarioTipoNavigation.UsuarioTipo)
        </dd>
         <form asp-action="Delete">
        <input type="hidden" asp-for="IdMenuTipoUsuario" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-secondary">Volver</a>
    </form>
    </dl>
    
   
</div>
