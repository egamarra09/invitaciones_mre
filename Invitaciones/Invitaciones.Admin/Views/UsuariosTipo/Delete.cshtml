@model Invitaciones.Shared.Data.UsuariosTipo

@{
    ViewData["Title"] = "Delete";
}

<div class="card ">
<div class="card-header">
    <h3 class="card-title">Usuario Tipo</h3>
</div>
 
    <dl class="card-body">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.UsuarioTipo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UsuarioTipo)
        </dd>
        <form asp-action="Delete">
            <input type="hidden" asp-for="IdUsuarioTipo" />
            <input type="submit" value="Eliminar" class="btn btn-danger" /> |
            <a asp-action="Index" class="btn btn-secondary">Volver</a>
        </form>
    </dl>
    
    
</div>
